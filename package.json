{
  "name": "webdriverio-tutorial",
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/node": "^20.11.13",
    "@wdio/allure-reporter": "^8.16.3",
    "@wdio/appium-service": "^8.5.9",
    "@wdio/cli": "^8.5.9",
    "@wdio/local-runner": "^8.5.9",
    "@wdio/mocha-framework": "^8.5.6",
    "@wdio/spec-reporter": "^8.4.0",
    "allure-playwright": "^2.12.0",
    "appium": "^2.0.1",
    "appium-uiautomator2-driver": "^2.43.4",
    "monocart-reporter": "^1.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wdio-vscode-service": "^5.0.0"
  },
  "scripts": {
    "build": "npm install",
    "wdio_android": "wdio config/wdio.android.conf.ts",
    "web_ui_test": "npx playwright test --update-snapshots --project=chromium --config=./uiTest.config.ts",
    "api_test": "npx playwright test --project=api_test --config=./apiTest.config.ts",
    "generate_webui_report" : "allure generate results/test-results/webui-allure-results -o results/allure-report --clean",
    "generate_api_report" : "allure generate results/test-results/api-allure-results -o results/allure-report --clean",
    "generate_mobile_report" : "allure generate results/test-results/mobile-allure-results -o results/allure-report --clean",
    "open_report" : "allure open results/allure-report"
  }
}
